.partnership_view .sec1 .sec_inner1 .roadmap_lst li:before{position:absolute; left:0; top:-3px; width:47px; box-sizing:border-box; font-family:'Exo'; font-size:50px; text-align:center; line-height:1; color:#b5afa0;}
