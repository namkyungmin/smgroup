@charset "utf-8";

@font-face {
	font-family: 'AritaBuriM';
	font-style: normal;
	font-weight: 400;
	src: url(/resource/eng/font/Arita-buriM.woff) format('woff');
}
@font-face {
	font-family: 'Exo';
	font-style: normal;
	font-weight: 400;
	src:url(/resource/eng/font/Exo-Regular.woff) format('woff');
} 
@font-face {
	font-family: 'Exo';
	font-style: normal;
	font-weight: 600;
	src: url(/resource/eng/font/Exo-Medium.woff) format('woff');
} 


/* common */
#container #contents{padding-top:66px;}
#header .header_wrap{background:#fff; border-bottom:1px solid #8a1c22; box-sizing:initial;}
#header .header_wrap .logo a{background-image:url(/resource/eng/mobile/images/common/logo.png);}
#header .header_wrap .btn_menu {background-image:url(/resource/eng/mobile/images/common/btn_menu_black.png);}
#header .header_wrap .btn_subsidiary{border-color:#d3dbdf; color:#000000; font-weight:700;}

.sustainability_view{word-break:keep-all;}
.sustainability_view .section{position:relative; width:100%; padding:0; margin-bottom:100px; box-sizing:border-box;}
.sustainability_view .section .img{max-width:100%;}
.sustainability_view .anchor_wrap{overflow:hidden; position:relative;}
.sustainability_view .section:last-of-type{margin-bottom:0;}
.sustainability_view span.anchor{position:absolute; left:0; top:-100px;}
.sustainability_view .sec_inner{margin:0 auto;}
.sustainability_view .sec_inner:after{display:block; clear:both; content:"";}
.sustainability_view .sec_tit{}
.sustainability_view .sec_tit h2{font-family:'Exo'; font-size:12px; font-weight:400; line-height:30px;}
.sustainability_view .sec_txt{margin-bottom:40px;}
.sustainability_view .sec_txt .txt1{margin-bottom:35px; font-size:20px; line-height:27px; color:#333;}
.sustainability_view .sec_txt .txt2{font-family:'AritaBuriM'; font-size:15px; font-weight:400; line-height:25px; color:#666;}
.sustainability_view .btn_default{position:relative; display:inline-block; padding:24px 20px 24px 25px; background:#fff; border:1px solid #d3dbdf; font-size:13px; line-height:1; text-align:left; color:#41545f;}
.sustainability_view .btn_default:after{display:inline-block; width:12px; height:12px; margin-left:10px; background:url(/resource/eng/mobile/images/sustainability/btn_ico.png) 0 0 no-repeat; background-size:24px; vertical-align:middle; content:"";}
.sustainability_view .download_btn:after{background-position:0 0;}
.sustainability_view .link_btn:after{background-position:-12px 0;}
.sustainability_view .slide_wrap{position:relative; border-top:2px solid #cabda8;}
.sustainability_view .slide_wrap:after{position:absolute; left:0; bottom:0; width:calc(100% - 95px); height:2px; content:"";}
.sustainability_view .slide_cont{padding-top:29px;}
.sustainability_view .slide_cont dt{margin-bottom:16px; font-size:18px;}
.sustainability_view .slide_cont dd{font-family:'AritaBuriM'; font-size:15px; line-height:20px;}
.sustainability_view .slide_control{overflow:hidden; position:absolute; right:0; bottom:-15px; z-index:5; line-height:1; }
.sustainability_view .slide_control .autoplay_ctrl{position:absolute; left:-9999px; top:0;}
.sustainability_view .slide_control > a{overflow:hidden; position:absolute; top:50%; margin-top:-8px; width:8px; height:13px; background-size:16px auto; vertical-align:middle; text-indent:-9999px;}
.sustainability_view .slide_control > a.slide_prev{left:0;}
.sustainability_view .slide_control > a.slide_next{right:0; background-position: 100% 0;}
.sustainability_view .slide_control .counter{display:inline-block; width:68px; padding:0 8px; font-family:'Exo'; font-size:16px; text-align:center; vertical-align:middle;}
.sustainability_view .slide_control .swiper-pagination-current{vertical-align:5px;}
.sustainability_view .slide_control .swiper-pagination-total{vertical-align:-5px;}
.sustainability_view [data-motion=bgType1]{transition:background 0.5s linear;}

/* ethics */
.ethics_view .sec1 .sec_inner1{width:100%; margin-bottom:40px;}
.ethics_view .sec1 .sec_inner1 .sec_tit{position:relative;}
.ethics_view .sec1 .sec_inner1 .sec_tit h2{position:absolute; top:10%; left:0; width:100%; padding-left:8.666%; text-align:center; letter-spacing:9px; line-height:1; color:#37608e;}
.ethics_view .sec1 .sec_inner1 .img_box{overflow:hidden;}
.ethics_view .sec1 .sec_inner1 .sec_txt{padding:50px 6.666% 40px 6.666%;}
.ethics_view .sec1 .sec_inner2 h3{position:relative; padding-top:42.5%; margin-bottom:10px; background:url(/resource/mobile/images/sustainability/ethics/view_sec1_bg1.jpg) center 0 no-repeat; background-size:100% auto; font-size:20px; text-align:center; color:#fff;}
.ethics_view .sec1 .sec_inner2 h3 span{position:absolute; left:0; top:50%; width:100%; text-align:center; transform:translate(0,-50%);}
.ethics_view .sec1 .sec_inner2 .practice_lst{overflow:hidden; padding:0 6.6666%;}
.ethics_view .sec1 .sec_inner2 .practice_lst li{position:relative; padding:30px 0 30px 48px; border-bottom:1px solid #93c69b; font-family:'AritaBuriM'; font-size:15px; text-align:left; line-height:25px;}
.ethics_view .sec1 .sec_inner2 .practice_lst li:before{content:"1"; position:absolute; left:0; top:30px; width:48px; font-family:'Exo'; font-size:49px; text-align:center; line-height:1; color:#b5afa0;}
.ethics_view .sec1 .sec_inner2 .practice_lst li:nth-child(1):before{content:"1";}
.ethics_view .sec1 .sec_inner2 .practice_lst li:nth-child(2):before{content:"2";}
.ethics_view .sec1 .sec_inner2 .practice_lst li:nth-child(3):before{content:"3";}
.ethics_view .sec1 .sec_inner2 .practice_lst li:nth-child(4):before{content:"4";}

.ethics_view .sec2 .sec_inner1{padding:0 6.6666%; margin-bottom:40px;}
.ethics_view .sec2 .sec_inner1 .sec_tit{position:relative;}
.ethics_view .sec2 .sec_inner1 .sec_tit h2{position:absolute; top:22px; right:22px; letter-spacing:7px; line-height:1; color:#7a785f; writing-mode: vertical-lr; -webkit-writing-mode: vertical-lr; -ms-writing-mode: tb-lr;}
.ethics_view .sec2 .sec_inner1 .img_box{overflow:hidden;}
.ethics_view .sec2 .sec_inner1 .sec_txt{}
.ethics_view .sec2 .sec_inner2{margin-top:-126px; background:url(/resource/mobile/images/sustainability/ethics/view_sec2_bg1.jpg) center 0 no-repeat; background-size:cover;}
.ethics_view .sec2 .sec_inner2 .inner{padding:116px 6.6666% 50px; margin:0 auto;}
.ethics_view .sec2 .sec_inner2 h3{margin-bottom:24px; font-family:'Exo'; font-size:20px; font-weight:400; line-height:1; color:#9e8562;}
.ethics_view .sec2 .sec_inner2 .slide_wrap{border-color:#cabda8;}
.ethics_view .sec2 .sec_inner2 .slide_wrap:after{background:#cabda8;}
.ethics_view .sec2 .sec_inner2 .slide_cont{min-height:190px;}
.ethics_view .sec2 .sec_inner2 .slide_cont dt{color:#4b3016;}
.ethics_view .sec2 .sec_inner2 .slide_cont dd{color:#6e5940;}
.ethics_view .sec2 .sec_inner2 .slide_control{}
.ethics_view .sec2 .sec_inner2 .slide_control > a{background-image:url(/resource/mobile/images/sustainability/ethics/slide_arr.png);}
.ethics_view .sec2 .sec_inner2 .slide_control .counter{color:#7c5f3a;}

.ethics_view .sec3{padding:0 6.6666%;}
.ethics_view .sec3 .sec_inner1{margin-bottom:40px;}
.ethics_view .sec3 .sec_inner1 .sec_txt{}
.ethics_view .sec3 .sec_inner2 .cont{position:absolute; left:0; top:0; position:relative; padding-top:165%; background:#171916 url(/resource/mobile/images/sustainability/ethics/view_sec3_bg1.jpg) center 0 no-repeat; background-size:100% auto;}
.ethics_view .sec3 .sec_inner2 h2{position:absolute; left:6.6666%; top:5%; font-family:'Exo'; font-size:12px; font-weight:400; line-height:21px; letter-spacing:6px; color:#b4814f;}
.ethics_view .sec3 .sec_inner2 .motion{position:absolute; left:0; bottom:0; padding: 5% 9.6% 15%; text-align:right;}
.ethics_view .sec3 .sec_inner2 h3{margin-bottom:20px; font-family:'Exo'; font-size:20px; font-weight:400; line-height:1; color:#7c7169;}
.ethics_view .sec3 .sec_inner2 .txt1{font-family:'AritaBuriM'; font-size:15px; line-height:30px; color:#7c7169;}
.ethics_view .sec3 .sec_inner2{margin-bottom:50px;}
.ethics_view .sec3 .btn_area{text-align:center;}

/* partnership */
.partnership_view #container #contents{padding-bottom:0;}
.partnership_view .sec1{padding:0 6.6666%;}
.partnership_view .sec1 .sec_inner1{}
.partnership_view .sec1 .sec_inner1 .sec_tit{position:relative; margin-bottom:50px;}
.partnership_view .sec1 .sec_inner1 .sec_tit h2{position:absolute; top:30px; left:25px; letter-spacing:8.5px; line-height:1; color:#777777; writing-mode: vertical-lr; -webkit-writing-mode: vertical-lr; -ms-writing-mode: tb-lr;}
